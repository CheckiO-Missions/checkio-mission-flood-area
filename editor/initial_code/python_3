from typing import List


def flood_area(diagram: str) -> List[int]:
    return []

if __name__ == '__main__':
    print("Example:")
    print(flood_area(r'\\//'))
    assert flood_area(r'\\//') == [4], 'valley'
    assert flood_area(r'/\\///\_/\/\\\\/_/\\///__\\\_\\/_\/_/') == [4, 2, 1, 19, 9], 'mountains'
    assert flood_area(r'_/-\_') == [], 'hill'

